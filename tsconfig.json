{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/shared/*": [
        "src/shared/*"
      ],
      "@/entities/*": [
        "src/entities/*"
      ],
      "@/features/*": [
        "src/features/*"
      ],
      "@/widgets/*": [
        "src/widgets/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/core/*": [
        "src/core/*"
      ],
      "@/services/*": [
        "src/services/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ]
}
